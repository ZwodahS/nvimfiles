{
  "group": {
    "prefix": "l.group",
    "body": [
      "<group id=\"${1:id}\">",
      "\t$2",
      "</group>"
    ]
  },
  "text": {
    "prefix": "l.text",
    "body": [
      "<text id=\"${1:id}\">",
      "\t<en>$2</en>",
      "</text>"
    ]
  },
  "en": {
    "prefix": "l.en",
    "body": [
      "<en>$2</en>"
    ]
  },
  "tag": {
    "prefix": "<",
    "body": [
      "<${1:} $2></$1>"
    ]
  }
}
