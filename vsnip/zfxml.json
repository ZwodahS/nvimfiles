{
  "scalegrid": {
    "prefix": "component.scalegrid",
    "body": [
      "<scalegrid width=\"${1:100}\" height=\"${2:20}\" factoryId=\"${3:paper.white.2}\" />"
    ]
  },
  "layout-dynamic": {
    "prefix": "layout.dynamic",
    "body": [
      "<layout-dynamic width=\"${1:120}\" height=\"${2:60}\" id=\"${3:id}\">",
      "</layout-dynamic>"
    ]
  },
  "layout-vflow": {
    "prefix": "layout.vflow",
    "body": [
      "<layout-vflow spacing=\"${1:5}\" id=\"${2:id}\">",
      "</layout-vflow>"
    ]
  },
  "layout-hflow": {
    "prefix": "layout.hflow",
    "body": [
      "<layout-hflow spacing=\"${1:5}\" id=\"${2:id}\">",
      "</layout-hflow>"
    ]
  },
  "layer-empty": {
    "prefix": "component.layer-empty",
    "body": [
      "<layer-empty id=\"${id}\" />"
    ]
  },
  "if-exists": {
    "prefix": "ifexists",
    "body": [
      "<if-exists existsKey=\"${1:key}\">",
      "\t${2:}",
      "</if-exists>"
    ]
  },
  "object": {
    "prefix": "component.object",
    "body": [
      "<object object=\"${1:key}\"/>"
    ]
  },
  "textComponent": {
    "prefix": "component.text",
    "body": [
      "<text fontName=\"${1:display.1}\" textColor=\"${2:black.0}\" id=\"${3:id}\" stringId=\"${4:stringId}\" />"
    ]
  }
}
